{
    "name": "daviddlv/mindbaz-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle to provide a Mindbaz SwiftMailer service",
    "keywords": [
        "mindbaz",
        "swiftmailer",
        "symfony",
        "bundle"
    ],
    "authors": [
        {
            "name": "David DELEVOYE",
            "email": "david.delevoye@adeo.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MindbazBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.3.9",
        "swiftmailer/swiftmailer": "~4.0|~5.0",
        "lancasters/mindbaz-library": "dev-master",
        "symfony/config": "~2.7|~3.0",
        "symfony/dependency-injection": "~2.7|~3.0",
        "symfony/http-kernel": "~2.7|~3.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/LancasterSolutions/mindbaz-library"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
